!!! 5
%html.no-js
  = render 'head'

  %body{ :class => "#{ controller.controller_name }", "ng-app" => "editide"}
    = render 'chromeframe'
    / = render 'header'
    / = render 'flashes'

    = yield

    / = render 'footer'

    -# Javascript at the bottom for fast page loading
    = render 'javascripts'
